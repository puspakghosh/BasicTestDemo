<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.basictestdemo">

    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_health_app"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_health_app_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity
            android:exported="true"
            android:theme="@style/FullScreenTheme"
            android:name="com.example.basictestdemo.framework.mvp.app.splash.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:exported="true"
            android:theme="@style/AppTheme"
            android:name="com.example.basictestdemo.framework.mvp.app.login.LoginActivity"
            android:windowSoftInputMode="adjustResize">
        </activity>
        <activity
            android:label="@string/title_activity_main"
            android:theme="@style/AppTheme"
            android:name="com.example.basictestdemo.framework.mvp.app.dashboard.DashboardActivity">
        </activity>
    </application>

</manifest>